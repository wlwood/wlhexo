<header id="header">
  <div class="title">
    <h1><a href="<%- config.root %>"><%= config.pTitle %><font size="3px">&nbsp;<%= config.sTitle %></font></a></h1>
    <!--<p style="font-size:0.82em;margin-top:5px"><a  style='font-size:13px;font-style:normal' href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></p>-->
    <p style="font-size:0.82em;"><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></p>
  </div>
  <nav class="nav">
    <ul>
      <% for (var i in theme.menu){ %>
      <% if ("Home" == i) { %>
        <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
	  <% }  else {%>
        <li><a  href="<%- theme.menu[i] %>"><%= i %></a></li>
	  <% }  %>
      <% } %>
      <% if (theme.rss) { %>
        <li><a href="<%- theme.rss %>"><%- __('rss_title') %></a></li>
      <% } %>
    </ul>
    <div class="clearfix"></div>
  </nav>
  <div class="clearfix"></div>
</header>
